@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using JobWebsiteClone.Models
@model IEnumerable<Listing>

@foreach(var l in Model)
{
    <div class="panel">
        <h1>@Html.DisplayFor(modelL => l.JobTitle)</h1>
        <h4>@Html.DisplayFor(modelL => l.Location)</h4>
        <p>@Html.DisplayFor(modelL => l.Employer)</p>
    </div>
}


